<div class="row">
    <div class="col-md-6 col-md-offset-3 well">
        <form role="form signUpFrom">
            <div class="form-group">
                <label for="email">Email address:</label>
                <input required placeholder="email" name="email" type="email" class="form-control" ng-model="email" id="email">
            </div>
            <div class="form-group">
                <label for="pwd">Password:</label>
                <input required placeholder="password" name="password" type="password" class="form-control" ng-model="password" id="pwd">
            </div>
            <div class="form-group">
                <label for="fname">First Name:</label>
                <input required placeholder="first name" name="fname" type="text" class="form-control" ng-model="fname" id="fname">
            </div>
            <div class="form-group">
                <label for="lname">Last Name:</label>
                <input required name="lname" placeholder="last name" type="text" class="form-control" ng-model="lname" id="lname">
            </div>
            <div class="text-right">
                <button type="submit" ng-click="signUp()" class="btn btn-primary">Submit</button>
                <button class="btn btn-default"><a href="/">Cancel</a></button>
            </div>
        </form>
    </div>
</div>
